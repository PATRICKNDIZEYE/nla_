<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link
    rel="stylesheet"
    href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700&display=swap"
  />
  <link
    rel="stylesheet"
    href="https://fonts.googleapis.com/css?family=Calibri:300,400,500,600,700&display=swap"
  />
  <title>Report Title</title>
  <style>
    * { margin: 0; padding: 0; box-sizing: border-box; letter-spacing: .03rem;
    font-family: 'Calibri', sans-serif; } body { margin: 2rem 4rem;
    margin-right: 0; } ul { padding: 0; margin: 0; list-style-position: inside;
    } .list-none { list-style: none; } .low-contrast { font-size: .8rem; color:
    grey; } .capitalize { text-transform: capitalize; } .long-text { word-break:
    break-all; max-width: 5rem; overflow: hidden; text-overflow: ellipsis;
    /*white-space: nowrap;*/ } /********** * Header * **********/ header {
    display: flex; justify-content: space-between; align-items: center;
    margin-bottom: 2rem; } header .report-heading { display: flex; align-items:
    center; gap: 3rem; } header .title { font-size: 1.5rem; font-weight: 600;
    color: #179fd9; font-family: 'Poppins', sans-serif; } header .logo { width:
    auto; height: 4rem; } header .report-date span { font-style: italic;
    font-weight: 500; font-size: small; } /********** * Main * **********/ main
    { margin-bottom: 2rem; display: flex; flex-direction: column; } main table {
    width: 100%; border-collapse: collapse; } main table tr:nth-child(odd) > td,
    main table tr:nth-child(odd) > th { background-color: #ffffff; } main table
    tr:nth-child(even) > td, main table tr:nth-child(even) > th {
    background-color: #bfbfbf5f; } main table th { padding: 1rem .5rem;
    text-align: left; text-wrap: nowrap; border-right: 1px solid #bfbfbf;
    border-left: 1px solid #bfbfbf; border-bottom: 1px solid #bfbfbf;
    border-top: 1px solid #bfbfbf; } main table td { padding: .5rem .5rem;
    text-align: left; text-wrap: nowrap; border-right: 1px solid #bfbfbf;
    border-left: 1px solid #bfbfbf; border-bottom: 1px solid #bfbfbf; } main
    table tr:last-child td { border-bottom: 1px solid #bfbfbf; } main table tr
    .status > span { vertical-align: middle; font-size: 1.3rem; color: grey; }
    main table tr .status > span[data-state="processing"] { color: rgb(0, 210,
    0); } main table tr .status > span[data-state="rejected"] { color: red; }
    main table tr .status > span[data-state="closed"] { color: rgb(180, 180,
    180); } main table tr .status > span[data-state="open"] { color: rgb(60, 60,
    255); } /********** * Footer * **********/ footer { display: flex;
    flex-direction: column; align-items: flex-end; font-style: italic; }
  </style>
</head>
<body>
  <header>
    {{! Add your report header content here }}
    <div class="report-heading">
      <img
        class="logo"
        src="data:image/png;base64,"
        alt="logo"
      />
      <span class="title">NATIONAL LAND AUTHORITY REPORT</span>
    </div>
    <div class="report-date">
      <span>From
        {{FormatDate startDate "ddd DD MMM YYYY"}}
        To
        {{FormatDate endDate "ddd DD MMM YYYY"}}</span>
    </div>
  </header>

  <main>
    {{! Add your report main content here }}
    <table>
      <thead>
        <tr>
          <th>District</th>
          <th>Case ID</th>
          <th>Land UPI</th>
          {{! <th>
                        <ul class="list-none">
                            <li>Case ID</li>
                            <li class="low-contrast">Land UPI</li>
                        </ul>
                    </th> }}
          <th>Dispute Type</th>
          <th>Claimant</th>
          <th>Defendant</th>
          <th>Village Chief Phone</th>
          <th>Status</th>
        </tr>
      </thead>
      <tbody>
        {{#each disputes as |dispute|}}
          <tr>
            <td class="district-{{dispute.district}}">{{dispute.district}}</td>
            <td>{{dispute.claimId}}</td>
            <td>{{dispute.upiNumber}}</td>
            {{!-- <td>
                            <ul class="list-none">
                                <li>{{dispute.claimId}}</li>
                                <li class="low-contrast">{{dispute.upiNumber}}</li>
                            </ul>
                        </td> --}}
            <td>
              {{#if (eq dispute.disputeType "Amakimbirane ashingiye ku mbibi")}}
                Ashingiye ku mbibi
              {{else if
                (eq
                  dispute.disputeType
                  "Amakimbirane ashingiye ku ibarura rusange ry’ubutaka ryabaye hagati y’umwaka wa 2009-2013"
                )
              }}
                Ashingiye ku ibarura<br />rya 2009-2013
              {{/if}}
            </td>
            <td>
              <ul class="list-none">
                <li>{{dispute.claimant.profile.Surnames}}<br
                  />{{dispute.claimant.profile.ForeName}}</li>
                <li
                  class="low-contrast"
                >(+250){{dispute.claimant.phoneNumber}}</li>
              </ul>
            </td>
            <td>
              <ul class="list-none">
                <li>{{dispute.defendant.fullName}}</li>
                <li
                  class="low-contrast"
                >(+250){{dispute.defendant.phoneNumber}}</li>
              </ul>
            </td>
            <td>(+250){{dispute.villageLeaderPhone}}</td>
            <td class="status">
              <span data-state="{{dispute.status}}">•</span>
              {{dispute.status}}
            </td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </main>

  <footer>
    {{! Add your report footer content here }}
    <span>Generated {{dataLength}} records</span>
    <span>Generated by: {{creatorname}}</span>
    <span>Generated on: {{FormatDate today "ddd DD MMM YYYY"}}</span>
  </footer>
</body>
<script>
  const districts = ['nyamasheke', 'huye', 'bugesera', 'kirehe', 'nyaruguru',
  'nyarugenge', 'karongi', 'gicumbi', 'nyamagabe', 'rulindo', 'musanze',
  'rutsiro', 'gisagara', 'nyagatare', 'ngororero', 'kicukiro', 'burera',
  'kamonyi', 'ruhango', 'rubavu', 'kayonza', 'gatsibo', 'muhanga', 'rusizi',
  'ngoma', 'gakenke', 'nyanza', 'nyabihu', 'gasabo', 'rwamagana']; const
  uniqueDistricts = [...new Set(districts)]; districts.forEach(district => {
  const tds = Array.from(document.querySelectorAll(`.district-${district}`));
  tds.forEach((td, index) => { td.style.backgroundColor = 'transparent'; if
  (index !== 0) td.innerText = ''; if (index !== tds.length - 1)
  td.style.borderBottom = '0px solid transparent'; }); });
</script>